{
  "env": {
    "browser": true,
    "es6": true,
    "jest": true
  },
  "globals": {
    "ENV": true
  },
  "extends": ["airbnb-base", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "import/extensions": ["error", { "js": "never" }],
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "trailingComma": "es5"
      }
    ]
  }
}
